<head>
  <link rel="stylesheet" href="/css/booking2.css" />
</head>
<header class="parallax-section container-fluid" style="width: 100%">
  <div id="banner-image">
    <div class="home_header_info" id="header">
      <h2
        class="typewrite"
        data-period="2000"
        data-type='[ "HABIBI Come to India ", "Adventure awaits, go find it.", "Donâ€™t be a tourist, be a traveler.","Mountain Calling" ]'
      ></h2>
    </div>
  </div>
</header>

<!-- booking form  -->
<div class="row">
  <div id="booking" class="section">
    <div class="section-center">
      <div class="container">
        <div class="row">
          <div class="booking-form">
            <form action="/booking" method="POST">
              <div class="row no-margin">
                <div class="col-md-3">
                  <div class="form-group">
                    <span class="form-label" for="place">Destination</span>
                    <input
                      class="form-control"
                      type="text"
                      id="place"
                      placeholder="Country, ZIP, city..."
                      name="destination"
                    />
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="row no-margin">
                    <div class="col-md-5">
                      <div class="form-group">
                        <span class="form-label" for="startDate">Check In</span>
                        <input class="form-control" type="date"  id="startDate" name="startDate" required />
                      </div>
                    </div>
                    <div class="col-md-5">
                      <div class="form-group">
                        <span class="form-label" for="endDate">Check out</span>
                        <input class="form-control" id="endDate" name="endDate" type="date" required />
                      </div>
                    </div>
                    <div class="col-md-2">
                      <div class="form-group">
                        <span class="form-label">Guests</span>
                        <select class="form-control">
                          <option>1</option>
                          <option>2</option>
                          <option>3</option>
                        </select>
                        <span class="select-arrow"></span>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="col-md-3">
                  <div class="form-btn">
                    <button  type="submit" class="submit-btn">Check availability</button>
                  </div>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- packages section -->
<div class="container-fluid">
  <div class="container packages">
    <h1
      class="display-4 mb-5 mt-5"
      style="font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif"
    >
      <img
        src="https://img.freepik.com/free-vector/airplane-sky_1308-31202.jpg?w=740&t=st=1681982316~exp=1681982916~hmac=fd4f9695f55528259a3c44a498c70505b8131cb8c6aa6d64d2ec68b7f33dd8bf"
        style="width: 100px; height: 100px; background: transparent"
      />Explore Our Packages
    </h1>
    <div class="container">
      <div class="row">
        <%for(let item of packages){%>
        <div class="col-lg-4 col-sm-8 mb-4">
          <div
            class="card "
            data-aos="fade-up"
            data-aos-duration="3000"
     
            style="
              height: 700px;
              border-radius: 5px;
              box-shadow: 1px 2px 3px 4px rgba(12, 12, 12, 0.2);
            "
          >
            <img
              src="<%=item.img%>"
              alt=""
              class="card-img-top"
              style="height: 340px"
            />
            <div class="card-body">
              <h3 class="card-title"><%=item.name%></h3>
              <p class="card-text"><%=item.desc%></p>
              <a
                href="/packages/<%=item._id%>"
                class="btn btn-outline-success btn-sm"
                >Details</a
              >
              <a href="" class="btn btn-outline-danger btn-sm"
                ><i class="far fa-heart"></i
              ></a>
            </div>
          </div>
        </div>
        <%}%>
      </div>
    </div>
  </div>
</div>



